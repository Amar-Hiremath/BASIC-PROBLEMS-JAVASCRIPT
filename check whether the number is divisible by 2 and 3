// write a javascript program to find whether a number is divisible by 2 and 3 ?

let num = prompt("Enter a number")
num = Number.parseInt(num);
if (num%2==0  && num%3==0 ){
  console.log("the number you have entered is devisible by 2 and 3 ")
}
  else{
  console.log("the number you have entered is not devisible by 2 and 3 ")
    
  }
